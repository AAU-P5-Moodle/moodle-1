{{#js}}
    require(['mod_livequiz/delete_question'], (module) => module.init({{quizid}}, {{lecturerid}}, "{{url}}"));
    require(['mod_livequiz/edit_question'], (module) => module.init({{quizid}}, {{lecturerid}}));
{{/js}}
<ol id="saved_questions_list" style="padding-left: 20px; margin: 0;">
            {{#questions}}
                <li id="question_list_{{questionid}}">
                    <span class="question_list_text" data-id="{{questionid}}">{{questiontitle}}</span>
                    <button class="fas fa-cog edit-question-btn" data-id="{{questionid}}"></button>
                    <button class="fa-solid fa-trash-can delete-question" id="{{questionid}}_delete_button" data-id="{{questionid}}"></button>
                </li>
            {{/questions}}
</ol>